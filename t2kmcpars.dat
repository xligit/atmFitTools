$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$  PARAMETER FILE FOR FITS $
$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$  FORMAT:<type>,<name>=<value>;
$  FIRST CHARACTER DENOTES DATA TYPE
$  SUPPORTED TYPES:
$	'i' -> integer
$	'f' -> float
$	'd' -> double
$	's' -> string

$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$$ GLOBAL PARAMETERS $$$$$$$
i,nSamples=1;
i,nFVBins=6;
i,nComponents=1;
i,nAttributes=4;
i,nSysPars=0;
s,globalRootName=t2kmc_numu;
s,FVBinName0=wall0;
s,FVBinName1=wall1;
s,FVBinName2=wall2;
s,fQAttName0=fqelike;
s,fQAttName1=fqrcpar;
s,fQAttName2=fqpi0like;
s,fQAttName3=fqpi0mass;
s,fQAttName4=fqpi0towallmin;
s,fQAttName5=fqpi0photangle;
s,MCComponentName0=single_e;
s,MCComponentName1=single_mu;
s,MCComponentName2=e_other;
s,MCComponentName3=mu_other;
s,MCComponentName4=pi0;
s,MCComponentName5=other;
s,sampleName0=1se;
s,sampleName1=2se;
s,sampleName2=gt2se;

$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$$ PREPROC PARAMETERS $$$$$$
s,preProcessOutDir=./rootfiles/;
s,preProcessFilesMC=/nfs/data40/t2k/amissert/skdata/t2kmc/shimp_skdet13p90_fQv4r0/numu_numu/root/numu_*9*.root;
s,preProcessFilesData=/xxx*.root;
$$ CODE FOR FV BINNING
$$   0 -> SIMPLE (3 FV BINS BASED ON WALL CUTS)
$$   1 -> COSMIC (SORT BY SIDE VS TOP ENTERING)
$$   2 -> NO FV BINNING (ONE BIN)
$$   4 -> BIN USING FV HISTOS
i,preProcessFVBinning=4;
$$ MC COMPONENT DEFINITIONS
$$   0 -> VISIBLE+NEUT
$$   1 -> COSMIC  (CUTS ON SUBEV #)
$$   2 -> VISIBLE (CUTS ONLY ON # VIS RINGS)
$$   3 -> HPI0 (NO CUTS)
i,preProcessMCComponents=0;
$$ MC AND DATA SAMPLE DEFINITION
$$  0 -> ATMOSPHERIC (BY NSE)
$$  1 -> COSMIC
$$  2 -> HYBRID PI0
i,preProcessMCSamples=0;
$$ NHITAC CUT FOR FC EVENTS
i,preProcFCCut=16;
$$ VISIBLE ENERGY CUT
d,preProcEVisCut=30.;
$$ MIN WALL CUT
d,preProcWallMinCut=0;
$$ MIN TOWALL Cut
d,preProcToWallMinCut=0;
$$ MAX SUBEVENTS
i,preProcNseMax=100;
$$ MIN SUBEVENTS
i,preProcNseMin=0;
$$ MIN TIME BETWEEN FIRST TWO SUBEVENTS (INGATE CUT)
d,preProcInGateCut=0;
$$ ADD ADDIONAL VARIABLES TO OUTPUT (MAY INCREASE PROCESSING TIME)
i,preProcAddMoreVars=0;

$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$$ HFACTORY PARAMETERS $$$$$
s,hFactoryOuputDir=./rootfiles/;
s,hFactoryOutput=./rootfiles/pi0_noloss_Efvbin.root;
s,hFactoryMCFiles=./rootfiles/pi0_noloss_Efvbin_ppmc*.root;
s,hFactoryDataFiles=./rootfiles/pi0_noloss_Efvbin_ppdat*.root;

$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$$ HISTOGRAM BINNING $$$$$$$
i,nBinsAtt0=25;
i,nBinsAtt1=25;
i,nBinsAtt2=25;
i,nBinsAtt3=25;
i,nBinsAtt4=25;
i,nBinsAtt5=25;
i,nBinsAtt6=25;
